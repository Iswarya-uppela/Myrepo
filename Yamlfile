apiVersion: apps/v1
kind: Deployment
metadata:
  name: MYAPP
spec:
  replicas: 1
  selector:
    matchLabels:
      app: MYAPP
  template:
    metadata:
      labels:
        app: MYAPP
    spec:
      containers:
      - name: MYAPP
        image: myregistry13.azurecr.io/MYAPP:latest
        ports:
        - containerPort: 80